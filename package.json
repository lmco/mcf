{
  "name": "mbee",
  "version": "0.1.0",
  "description": "Model-Based Engineering Environment",
  "main": "bin/server.js",
  "repository": "https://gitlab.lmms.lmco.com/mbee/mbee.git",
  "author": "Josh Kaplan <joshua.d.kaplan@lmco.com>",
  "license": "LMPI",
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.2",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.7",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "htmlspecialchars": "^1.0.5",
    "ldapjs": "^1.0.2",
    "mongo-sanitize": "^1.0.0",
    "mongodb": "^3.0.7",
    "mongoose": "^5.0.16",
    "winston": "^3.0.0-rc5"
  },
  "devDependencies": {
    "bootstrap": "^4.0.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-jsdoc3": "^1.0.1",
    "gulp-react": "^3.1.0",
    "gulp-sass": "^3.1.0",
    "jquery": "^3.3.1",
    "mocha": "^5.0.4",
    "popper.js": "^1.12.9",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "request": "^2.85.0",
    "swagger-jsdoc": "^1.9.7",
    "swagger-ui-express": "^2.0.15",
    "ub-jsdoc": "^1.6.0"
  },
  "peerDependencies": {},
  "scripts": {
    "build": "./node_modules/.bin/gulp",
    "build:jsdoc": "node node_modules/jsdoc/jsdoc.js -t node_modules/ub-jsdoc/ app/* && mv out docs",
    "build:jsdoc-old": "./node_modules/.bin/gulp jsdoc",
    "clean": "./node_modules/.bin/gulp clean",
    "clean:all": "rm -rf public/ node_modules/ docs/",
    "db": "mongod --auth --dbpath ./db/mbee",
    "docker:build": "yarn install && yarn build && docker build -t mbee .",
    "docker:run:dev": "docker run -d -it --restart=always -p 9080:9080 -p 9443:9443 --name mbee-dev mbee",
    "docker:run": "docker run -d -it --restart=always -p 80:9080 -p 443:9443 --name mbee mbee",
    "server": "node ./bin/server.js",
    "server:jsdoc": "./node_modules/.bin/gulp jsdoc && node ./bin/jsdoc-server.js",
    "start": "yarn install && yarn run build && yarn run server",
    "test": "./node_modules/.bin/mocha test/_orchestrator.js --slow 19"
  }
}
